    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.6",
    "eslint-config-prettier": "^4.2.0",
    "eslint-plugin-prettier": "^3.0.1",
    "nodemon": "1.19.0",
    "prettier": "^1.17.0",
    "webpack": "^4.35.0",
    "webpack-cli": "^3.3.5"
  }
}
