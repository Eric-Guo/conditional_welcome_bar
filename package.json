{
  "name": "conditional_welcome_bar",
  "version": "1.0.0",
  "private": true,
  "description": "Shopify app with Rails API and React. Utilizes Shopify's Polaris UI components.",
  "scripts": {
    "setup": "npm --prefix ui install",
    "build": "npm run setup && npm --prefix ui run build",
    "deploy": "cp -a ui/build/. public/",
    "deploy:local": "npm --prefix ui run build && npm run deploy",
    "webpack": "webpack --config webpack.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/juliantrueflynn/conditional_welcome_bar.git"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/juliantrueflynn/conditional_welcome_bar/issues"
  },
  "homepage": "https://github.com/juliantrueflynn/conditional_welcome_bar#readme",
  "dependencies": {
    "@shopify/app-bridge": "1.3.1-next.2",
    "@shopify/polaris": "3.18.0",
    "@shopify/react-shopify-app-route-propagator": "3.0.0",
    "apollo-boost": "^0.3.1",
    "graphql": "^14.3.0",
    "hex-to-rgb": "^1.0.1",
    "history": "^4.9.0",
    "humps": "^2.0.1",
    "lodash.isequal": "^4.5.0",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-apollo": "^2.5.5",
    "react-dom": "^16.8.6",
    "react-router-dom": "^5.0.0"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
